{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "sil",
      "camelcase": "Sil",
      "title": "Simple Issue Language",
      "scope": "source.sil",
      "file-types": [
        "sil"
      ],
      "injection-regex": "^sil$",
      "class-name": "TreeSitterSil"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "A parser for the Simple Issue Language developed by Appfire",
    "authors": [
      {
        "name": "Jani Bangiev",
        "email": "jani.bangiev@gmail.com",
        "url": "https://www.linkedin.com/in/jani-bangiev-510366220/"
      }
    ],
    "links": {
      "repository": "https://github.com/Term164/tree-sitter-sil.git"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}